datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

// Define your own datamodels here and run `yarn redwood prisma migrate dev`
// to create migrations for them and apply to your dev DB.


  public.contactBookingForm (
    id bigint generated by default as identity not null,
    created_at timestamp with time zone null default now(),
    firstName character varying null default ''::character varying,
    lastName character varying null default ''::character varying,
    email character varying null default ''::character varying,
    address character varying null default ''::character varying,
    contactNumber character varying null default ''::character varying,
    message character varying null default ''::character varying,
    timeBooking time without time zone null default now(),
    dateBooking date null,
    constraint contactBookingForm_pkey primary key (id)
  ) tablespace pg_default;
